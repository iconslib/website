    "prettier-plugin-tailwindcss": "^0.5.14",
    "svelte": "^5.0.0-next.115",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.2.0"
